{
	"name": "qiankun-demo",
	"version": "0.0.1",
	"description": "umi plugin for qiankun",
	"repository": "umijs/umi-plugin-qiankun",
	"main": "index.js",
	"keywords": [
		"umi-plugin",
		"micro frontend",
		"qiankun"
	],
	"engines": {
		"node": ">=8.0.0"
	},
	"scripts": {
		"start": "npm-run-all --parallel start:*",
		"install": "npm-run-all --parallel install:*",
		"start:master": "cd master && umi dev",
		"start:app1": "cd app1 && umi dev",
		"install:master": "cd master && yarn",
		"install:app1": "cd app1 && yarn"
	},
	"dependencies": {
		"umi": "^3.5.13"
	},
	"devDependencies": {
		"npm-run-all": "^4.1.5"
	}
}
